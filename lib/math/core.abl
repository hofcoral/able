fun add(a, b):
    return a + b

fun sqrt(x):
    guess = x / 2
    epsilon = 1 / 100000
    while abs(guess * guess - x) > epsilon:
        guess = (guess + x / guess) / 2
    return guess

fun floor(x):
    i = int(x)
    if i > x:
        return i - 1
    return i

fun ceil(x):
    i = int(x)
    if i < x:
        return i + 1
    return i

fun round(x):
    if x >= 0:
        return int(x + 1 / 2)
    return int(x - 1 / 2)
