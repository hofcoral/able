class Person():

    set init to (this, first_name, last_name, age):
        this.first_name to first_name
        this.last_name to last_name
        this.age to age

    
    set greet to (this):
        pr("Hello from ", this.first_name, "!")

set person to Person("Donald", "Draper", 45)

person.greet()
